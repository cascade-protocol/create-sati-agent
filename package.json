{
  "name": "create-sati-agent",
  "version": "0.1.1",
  "description": "CLI for AI agent identity on Solana - register, discover, review, and check reputation via SATI",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "create-sati-agent": "./dist/bin/cli.js"
  },
  "scripts": {
    "build": "npm run clean && tsdown --publint",
    "dev": "tsdown --watch",
    "type-check": "tsc --noEmit",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "check": "npm run type-check && npm run lint",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@stricli/core": "^1.2.4",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/node": "^22.0.0",
    "publint": "^0.3.17",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.0"
  },
  "files": [
    "dist/**",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "solana",
    "ai-agent",
    "identity",
    "sati",
    "erc-8004",
    "reputation",
    "x402"
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cascade-protocol/create-sati-agent.git"
  },
  "homepage": "https://github.com/cascade-protocol/create-sati-agent#readme",
  "bugs": {
    "url": "https://github.com/cascade-protocol/create-sati-agent/issues"
  }
}
